{
  "obsidianCss": true,
  "defaultViewMode": "source",
  "tabSize": 2,
  "attachmentFolderPath": "./attach",
  "promptDelete": false,
  "pdfExportSettings": {
    "pageSize": "A4",
    "landscape": false,
    "margin": "1",
    "downscalePercent": 100
